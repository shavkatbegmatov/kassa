[32m+[m
[32m+[m[32mif (isset($_SESSION['user'])) {[m
[32m+[m[32m    if ($_SESSION['user']['role'] != 'admin') {[m
[32m+[m[32m        if ($_SESSION['user']['role'] != 'user') {[m
